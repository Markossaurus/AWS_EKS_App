pipeline{
    agent{
        label "master"
    }
    stages{
        stage("git"){
            steps{
                echo "cloning repo from Github..."
                git credentialsId: 'edd5b572-22c2-43c1-a50b-991127524391', url: 'git@github.com:Markossaurus/AWS_EKS_App.git'
                }
            }
        }
        stage("build"){
            agent {
                dockerfile{
                    filename 'Dockerfile'
                    dir 'NginxCostum1'
                    label 'test'
                }  
            }
            steps{
                echo "image built"
            }
        }
}
